{   
    "name": "knn",
    "class": "sklearn.neighbors.KNeighborsClassifier",
    "version": "0.18.2",
    "parameters": {
        "n_neighbors": {
           "type": "int",
           "categorical": false,
           "range": [1, 20]
        },
        "weights": {
           "type": "string",
           "categorical": true,
           "range": ["uniform", "distance"]
        },
        "algorithm": {
           "type": "string",
           "categorical": true,
           "range": ["ball_tree", "kd_tree", "brute"]
        },
        "leaf_size": {
           "type": "int",
           "categorical": false,
           "range": [1, 50]
        },
        "metric": {
            "type": "string",
            "categorical": true, 
            "range": ["minkowski", "euclidean", "manhattan", "chebyshev"]
        },
        "p": {
           "type": "int",
           "categorical": false,
           "range": [1, 3]
        },
        "_scale": {
           "type": "bool",
           "categorical": true,
           "range": [true]
        }
    },
    "root_parameters": ["n_neighbors", "weights", "algorithm", "metric", "_scale"],
    "conditions": {
        "metric": {
            "minkowski": ["p"]
        },
        "algorithm": {
            "ball_tree": ["leaf_size"],
            "kd_tree": ["leaf_size"]
        }
    }
}
