{   
    "name": "pa",
    "class": "sklearn.linear_model.PassiveAggressiveClassifier",
    "parameters": {
        "C": {
           "type": "float_exp",
           "range": [10e-5, 10e5]
        },
        "fit_intercept": {
           "type": "int_cat",
           "range": [0, 1]
        },
        "n_iter": {
           "type": "int",
           "range": [10, 200]
        },
        "shuffle": {
           "type": "bool",
           "range": [true]
        },
        "loss": {
            "type": "string",
            "range": ["hinge", "squared_hinge"]
        },
        "_scale": {
           "type": "bool",
           "range": [true]
        },
        "n_jobs": {
           "type": "int_cat",
           "range": [-1]
        }
    },
    "root_parameters": ["C", "fit_intercept", "n_iter", "shuffle", "loss", "_scale", "n_jobs"],
    "conditions": {}
}
